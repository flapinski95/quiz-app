generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserProfile {
  id         String   @id @default(uuid())
  keycloakId String   @unique
  avatarUrl  String?
  bio        String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}